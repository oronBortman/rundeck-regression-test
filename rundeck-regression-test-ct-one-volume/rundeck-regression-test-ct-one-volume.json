      "checkout": "origin/oron/ct/idss/check-refactor_idss_with_inference_fixes-dual"
    },
    {
      "target": "src/screening",
      "repo": "git@github.com:SeeTrueAI/screening.git",
      "checkout": "oron/ct/idss/inference_fixes-dual"
    },
    {
      "target": "src/database",
      "repo": "git@github.com:SeeTrueAI/DataBase.git",
